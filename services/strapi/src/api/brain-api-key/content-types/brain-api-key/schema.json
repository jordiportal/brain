{
  "kind": "collectionType",
  "collectionName": "brain_api_keys",
  "info": {
    "singularName": "brain-api-key",
    "pluralName": "brain-api-keys",
    "displayName": "Brain API Key",
    "description": "API keys para acceso externo a Brain (OpenAI-compatible API)"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "key": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "keyPrefix": {
      "type": "string"
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "permissions": {
      "type": "json",
      "default": {
        "models": ["brain-adaptive", "brain-chat", "brain-rag"],
        "maxTokensPerRequest": 4096,
        "rateLimit": 60
      }
    },
    "usageStats": {
      "type": "json",
      "default": {
        "totalRequests": 0,
        "totalTokens": 0,
        "lastUsed": null
      }
    },
    "expiresAt": {
      "type": "datetime"
    },
    "createdByUser": {
      "type": "string"
    },
    "notes": {
      "type": "text"
    }
  }
}
