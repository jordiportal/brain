{
  "kind": "collectionType",
  "collectionName": "code_executions",
  "info": {
    "singularName": "code-execution",
    "pluralName": "code-executions",
    "displayName": "Code Execution",
    "description": "Historial de ejecuciones de c√≥digo generado"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "executionId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "query": {
      "type": "text",
      "required": true
    },
    "language": {
      "type": "enumeration",
      "enum": ["python", "javascript"],
      "required": true
    },
    "generatedCode": {
      "type": "text",
      "required": true
    },
    "executionOutput": {
      "type": "json"
    },
    "status": {
      "type": "enumeration",
      "enum": ["success", "error", "timeout", "container_error"],
      "required": true
    },
    "attempts": {
      "type": "integer",
      "default": 1
    },
    "executionTime": {
      "type": "decimal"
    },
    "exitCode": {
      "type": "integer"
    },
    "stdout": {
      "type": "text"
    },
    "stderr": {
      "type": "text"
    },
    "errorMessage": {
      "type": "text"
    },
    "containerId": {
      "type": "string"
    },
    "brainChain": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brain-chain.brain-chain"
    },
    "metadata": {
      "type": "json"
    }
  }
}
